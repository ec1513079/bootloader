{
  "manifest_version": 2,

  "name": "Bootloader",
  "description": "Bootloader description.",
  "version": "1.0",

  "icons":
  {
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },

  "background" : {
    "scripts": ["setting_util.js","browser_icon_script.js"]
  },
  "browser_action":
  {
    "default_icon" : "img/icon-19-disable.png",
    "default_title": "Bootloader"
  },
  "content_scripts": [
    {
      "matches": [
                "https://www.google.co.jp/search?*"
            ],
            "js": ["jquery-1.9.0.min.js","content_script.js"],
            "run_at": "document_end"
      }
  ],

  "permissions": [
    "tabs",
    "http://*/",
    "httpd://*/"
  ]
}
